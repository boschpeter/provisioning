{
  "restApiRoot": "/api",
  "host": "127.0.0.1",
  "port": 3000,
  "resetHost": "127.0.0.1",
  "resetPort": 80,
  "remoting": {
    "errorHandler": {
      "disableStackTrace": true
    },
    "context": false,
    "rest": {
      "handleErrors": false,
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false
  },
  "legacyExplorer": false
}
